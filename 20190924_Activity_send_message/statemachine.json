{
    "StartAt": "GetFile",
    "States": {
        "GetFile": {
            "Type": "Task",
            "Resource": "${FunctionGet.Arn}",
            "Next": "SendMessage",
            "InputPath": "$"
        },
        "SendMessage": {
            "Type": "Task",
            "Resource": "${Queue.Arn}",
            "Parameters": {
                "QueueUrl": "https://sqs.ap-northeast-1.amazonaws.com/${AWS::AccountId}/${Queue.QueueName}",
                "MessageBody.$": "$"
            },
            "End": true
        }
    }
}